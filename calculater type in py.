num1 = int(input("Enter first number : "))
num2 = int(input("Enter second number : "))
op = input("enter mathametical operato : ")

if(op=="+"):
    print("result is ",num1+num2)
elif(op=="-"):
    print("result is ",num1-num2)
elif(op=="*"):
    print("result is ",num1*num2)
elif(op=="//"):
    print("result is ",num1//num2)
if(op=="%"):
    print("result is ",num1%num2)
    
