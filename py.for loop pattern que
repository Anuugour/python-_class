for i in range(1,7):
    for j in range(1,7):
     if(i%2==0):
        print("#",end = " ")
     else:
        print("*",end=" ")
    print()    


a = 5
for i in range(1,a+1):
    for j in range(1,i+1):
        print("#",end = " ")
    print()




for i in range(1,6):
    print("*"*i)



a = 5
for i in range(1,a+1):
    for j in range(1,i+1):
        print(j,end = " ")
    print()
    
    
a = 5
