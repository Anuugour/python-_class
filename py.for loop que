num = int(input("enter a number: "))
sum = 0
for i in range(1,num+1 ):
    sum = sum+i
print("total sum =  ",sum)


    
n = int(input("Enter a number "))
sum =0
for i in range(1,n+1):
    sum = sum +(i*i)
print("sum of square = ",sum)



n = int(input("Enter a number "))
sum = 0
for i in range(1,n+1):
    sum = sum +(i*i*i)
print("sum of square = ",sum)


